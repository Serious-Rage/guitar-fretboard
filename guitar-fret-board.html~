<link rel="import" href="../polymer/polymer-element.html">

<dom-module id="guitar-fret-board">
  <template>
    <style>
      :host {
        display: block;
      }
    </style>
    <h2>Hello [[prop1]]!</h2>
  </template>

  <script>
    /**
     * `guitar-fret-board`
     * a guitar fret board graphic for displaying chord shaps
     *
     * @customElement
     * @polymer
     * @demo demo/index.html
     */
    class GuitarFretBoard extends Polymer.Element {
      static get is() { return 'guitar-fret-board'; }
      static get properties() {
        return {
          prop1: {
            type: String,
            value: 'guitar-fret-board'
          }
        };
      }
    }

    window.customElements.define(GuitarFretBoard.is, GuitarFretBoard);
  </script>
</dom-module>
